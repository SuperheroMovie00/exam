.scr-btn { padding: 0 12px; height: 24px; line-height: 22px; vertical-align: top; border: 0; }
.scr-btn:active { background: #00bee7; box-shadow: inset 0px 1px 2px 2px #00a6cb; position: relative; }
.scr-active { background: #00bee7; box-shadow: inset 0px 1px 2px 2px #00a6cb; position: relative; }
.disabled { background: #b6b6b6 !important; color: #f3f3f3 !important; }
/*主体开始*/
.main { width: 100%; height: 100%; min-width: 1280px; background: #fff; box-sizing: border-box; padding: 50px 0 0 200px; position: relative; }
.header { width: 100%; height: 50px; background: #fff; box-shadow: 0px 2px 2px 0px rgba(4, 4, 4, 0.09); position: absolute; left: 0; top: 0; z-index: 101; box-sizing: border-box; padding-left: 230px; }
.header .sys-logo { display: block; width: 422px; height: 48px; background: url(../images/sys-logo.png) no-repeat left 0px; }
.header .user-info { height: 42px; line-height: 42px; font-size: 14px; margin-top: 2px; padding-right: 30px; }
.header .user-info .uhead { display: inline-block; height: 36px; width: 36px; vertical-align: middle; margin: -2px 15px 0 10px; }
.header .user-info .uhead img { width: 100%; height: 100%; border-radius: 100%; border: 1px solid #00afe8; }
.header .user-info .quit i { font-size: 18px; margin-left: 5px; }
/*导航*/
.nav { width: 200px; height: 100%; background: #2f3b4c; position: absolute; left: 0; top: 0; z-index: 102; box-sizing: border-box; padding: 88px 0 40px 0; }
.nav .logo { display: block; width: 200px; height: 50px; background: url(../images/logo.png) no-repeat 20px center #00afe8; background-size: 144px 38px; position: absolute; left: 0; top: 0; }
.nav .par-menu { width: 100%; position: absolute; left: 0; top: 50px; }
.nav .par-menu li { width: 100%; height: 36px; position: relative; -webkit-transition: all 0.3s ease-in-out; -moz-transition: all 0.3s ease-in-out; transition: all 0.3s ease-in-out; }
.nav .par-menu li a { color: #d6e5ed; display: block; font-size: 13px; overflow: hidden; line-height: 36px; padding-left: 20px; }
.nav .par-menu li a i { font-size: 18px; margin-right: 10px; }
.nav .par-menu li:hover, .nav .par-menu li.active { background: #2a3344; color: #00bfe8; }
.nav .par-menu li:hover a, .nav .par-menu li.active a { color: #00bfe8; }
.nav .par-menu li ul { position: absolute; left: 0; top: 43px; z-index: 1; width: 100%; background: #0d1118; padding-bottom: 5px; display: none; }
.nav .par-menu li ul li a { color: #d6e5ed !important; }
.nav .par-menu li ul li a:hover { color: #00bfe8 !important; background: #171e29 !important; }
.nav .par-sub-menu li { width: 100%; min-height: 32px; position: relative; -webkit-transition: all 0.3s ease-in-out; -moz-transition: all 0.3s ease-in-out; transition: all 0.3s ease-in-out; background: #283343; margin-bottom: 1px; }
.nav .par-sub-menu li .item { height: 32px; position: relative; }
.nav .par-sub-menu li .item a { color: #d6e5ed; display: block; font-size: 14px; overflow: hidden; line-height: 32px; padding-left: 40px; }
.nav .par-sub-menu li .item a i { font-size: 10px; position: absolute; left: 20px; top: 50%; margin-top: -7px; width: 14px; height: 14px; line-height: 14px; }
.nav .par-sub-menu li ul { display: none; }
.nav .par-sub-menu li ul { padding: 0px 0 8px 0; background: #2f3b4c; display: none; }
.nav .par-sub-menu li ul li { height: 22px; min-height: 22px; line-height: 22px; background: #2f3b4c; font-size: 13px; }
.nav .par-sub-menu li ul li a { color: #d6e5ed; display: block; padding-left: 22px; }
.nav .par-sub-menu li ul li a i { margin-right: 6px; font-size: 14px; }
.nav .par-sub-menu li ul li.bk10 { height: 6px; min-height: 6px; }
.nav .par-sub-menu li.active ul { display: block; }
.nav .par-sub-menu li.active .item a { color: #00afe8; background: #1f2837; }
.nav .par-sub-menu li.active .item a i { position: absolute; transform: rotate(90deg); -ms-transform: rotate(90deg); -webkit-transform: rotate(90deg); }
/*.nav .par-sub-menu li:hover .item a { color: #00afe8; background: #1f2837; }
.nav .par-sub-menu li:hover .item a i { position: absolute; transform: rotate(90deg); -ms-transform: rotate(90deg); -webkit-transform: rotate(90deg); }
.nav .par-sub-menu li:hover ul { display: block; }*/
/*滚动条样式设置*/
.par-sub-menu::-webkit-scrollbar {
 width: 16px;
 height: 16px;
 background-color:transparent;
}
/*定义滚动条的轨道，内阴影及圆角*/
.par-sub-menu::-webkit-scrollbar-track {
 /*-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);*/
 border-radius: 10px;
 background-color: transparent;
}
/*定义滑块，内阴影及圆角*/
.par-sub-menu::-webkit-scrollbar-thumb {
 /*width: 10px;*/
 height: 20px;
 border-radius: 10px;
