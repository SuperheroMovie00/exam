.hid { display: none; }
/*按钮设置*/
.btn { display: inline-block; padding: 6px 15px; font-size: 14px; font-weight: 400; text-align: center; white-space: nowrap; vertical-align: middle; cursor: pointer; -webkit-user-select: none; -moz-user-select: none; -ms-user-select: none; user-select: none; background: #a8a8a8; border: 1px solid transparent; border-radius: 4px; color: #fff; position: relative; outline: none }
