/*药方滑动选单*/
.presc-pop-menu{ width: 700px; height: 100%; position: fixed; right:-700px; top: 0; z-index: 1000; background: #fff; border-left: 2px solid #ffa800; box-sizing: border-box; padding: 15px; overflow-y: auto;}
.presc-pop-menu ul:hover{background: #e1e1e1;}

.kf-serach{ width: 100%; height: 36px; position: relative; border: 1px #ccc solid;}
.kf-serach .txt{ width: 100%; height: 36px; border: 0; box-sizing: border-box; padding: 0 15px; font-size: 14px;}
.kf-serach .btn{ width: 60px; height: 36px; position: absolute; right: 0; top: 0;  background: #ffa800;}
/*辅助开方*/
.kf-auxiliary{width: 50%; float: left;}
.kf-seletab .kft-content{width: 100%; box-sizing: border-box; border: 1px #ccc solid; padding:0; border-top: 0; position: relative; padding-left: 100px; min-height: 500px;}
.kf-seletab .kft-nav{border-bottom: 1px #ccc solid; width: 100%; height: 32px;}
.kf-seletab .kft-nav a{display: block; float: left; width: 100px; height: 32px; position: relative; text-align: center; line-height: 32px; top: 0px; font-size: 14px;}
.kf-seletab .kft-nav a.active{ border: 1px #ccc solid; border-bottom: 0; background: #fff; color: #0e90d2; }
.kft-content .diseasx{ width: 100px; text-align: center; height: 100%; border-right: 1px #e1e1e1 solid; box-sizing: border-box; position: absolute; left: 0; top: 0; height: 100%; overflow-y: auto;}
.kft-content .diseasx li a{display: block; height: 28px; line-height: 28px; border-bottom: 1px #e1e1e1 solid;}
.kft-content .diseasx li a:hover{background: #e1e1e1;}
.kft-content .diseasx li:last-child a{border-bottom: 0;}
.kft-content .diseasx li.active a{background: #e1e1e1;}
.kft-content .symptoms{padding: 12px 15px;}
.kft-content .symptoms .sym-serach .txt{width:100%; height: 34px; border: 1px #ccc solid; padding: 0px 10px; font-size: 14px; box-sizing: border-box;}
.kft-content .symptoms .sym-serach{ width: 320px; height: 34px; position: relative;}
.kft-content .symptoms .sym-serach ul{ position: absolute; width: 100%; left: 0; top: 34px; border: 1px #ccc solid; border-top: 0; background: #fff; box-sizing: border-box; padding:5px 0;}
.kft-content .symptoms .sym-serach ul li{line-height: 26px; font-size: 14px;}
.kft-content .symptoms .sym-serach ul li a{display: block; padding: 0px 15px; }
.kft-content .symptoms .sym-serach ul li a:hover{background: #00bee7; color: #fff;}
.kft-content .symptoms dl dt{font-size: 13px; font-weight: bold; padding: 5px 0;}
.kft-content .symptoms dl dd{overflow: hidden;}
.kft-content .symptoms dl dd .item{display: block; float: left; padding: 2px 8px; min-width: 50px; text-align: center; color: #424242; background: #e1e1e1; font-size: 13px; border-radius: 3px; margin-right: 8px; cursor: pointer;}
.kft-content .symptoms dl dd .item:hover{background: #ffa800; color: #fff;}
/*添加诊疗方案*/
.kf-faq dt .txtbox .txt2{width: 80%; box-sizing: border-box; height: 36px; padding: 0px 10px; line-height: 36px; border: 1px #ccc solid;}
.kf-faq dt .txtbox .txtarea{width: 80%; box-sizing: border-box; mh-height:72px; padding: 0px 10px; line-height: 36px; border: 1px #ccc solid;}
.kf-faq dt .methods{width: 80%; box-sizing: border-box; padding:0; line-height: 36px; border: 1px #ccc solid; margin-bottom: 10px;}
.kf-faq dt .methods .met-tit{overflow: hidden; border-bottom: 1px #ccc solid; height: 36px; line-height: 36px; padding: 0px 10px;}
.kf-faq dt .methods .met-tit .btn{line-height: 22px;}
.kf-faq dt .methods .met-content{padding:5px 10px; overflow: hidden;}
.kf-faq dt .methods .met-content .item{float: left; line-height: 28px; padding-right: 15px;}
/*表格便宜*/
.td-py500{ box-sizing: border-box;}
.tdpy-content{display: none;}
.td-py500 .table-in{width: 60%; float: left;}
.td-py500 .tdpy-content{width: 39%;float: left; display: block!important; background: #ccc; height: 100%;}

/*树页面新样式*/
.tree-nstyle{ border: 1px solid #e2e2e2; left: 10px; width: 270px; padding: 6px; }
.new-trees-info{padding-left: 280px; height: 100%;}
.new-trees-scroll{height: 100%;  box-sizing: border-box; padding-bottom: 78px; position: relative; }
.new-trees-scroll-in{height: 100%; overflow-y: auto;}
.new-trees-info .data-oper{position: absolute!important;}







