body{background:url(../images/bg.png) repeat-x 0 0 #3c8cc0; min-width:1366px; }
.header{ width:100%; min-width:1366px; height:40px; margin:0 auto; background:url(../images/head_bg.png) repeat-x 0 0; position:absolute; left:0; top:0; z-index:11;}
.header .icp{  line-height:40px; text-shadow: 0.5px 0.866px 1px #3a5b7a; color:#8ea9c7; padding-right:20px;}

/*导航*/
.main{ overflow:auto; position:relative; padding:40px 15px 0 100px;}
.menu{ width:86px; height:100%; background:#393c41; position:absolute; left:0; top:0; z-index:10;}
.menu ul{ display:block; padding:0 0 0 0; border-bottom:1px solid #57595d;}
.menu ul li{ display:block; text-align:center; border-bottom:1px solid #1b1b1b; border-top:1px solid #57595d;}
.menu ul li a{ display:block; color:#fff; padding:14px 0;}
.menu ul li a i{ display:block; height:22px; width:22px; background:url(../images/menu_ico.png) no-repeat; margin:0 auto 5px auto;}
.menu ul li a:hover{ color:#5ee6df; text-decoration:none; background:#051f1f; }
.menu ul li.current a{color:#5ee6df; }

.menu ul li a.home i{}
.menu ul li a.home:hover i{ background-position:-56px 0;}
.menu ul li.current a.home i{ background-position:-56px 0;}

.menu ul li a.log i{ background-position:0 -45px;}
.menu ul li a.log:hover i{ background-position:-56px -45px;}
.menu ul li.current a.log i{ background-position:-56px -45px;}

.menu ul li a.security i{ background-position:0 -86px;}
.menu ul li a.security:hover i{ background-position:-56px -86px;}
.menu ul li.current a.security i{ background-position:-56px -86px;}


.menu ul li a.document i{ background-position:4px -128px; width:30px; height:26px;}
.menu ul li a.document:hover i{ background-position:-52px -128px;}
.menu ul li.current a.document i{ background-position:-52px -128px; }

.menu ul li a.order i{ background-position:0 -171px; width:26px; height:26px;}
.menu ul li a.order:hover i{ background-position:-56px -171px;}
.menu ul li.current a.order i{ background-position:-56px -171px;}

.menu ul li a.exit i{ background-position:0 -215px; width:26px; height:26px;}
.menu ul li a.exit:hover i{ background-position:-56px -215px;}
.menu ul li.current a.exit i{ background-position:-56px -215px;}
/*导航结束*/


