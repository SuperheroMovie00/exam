.main { width: 100%; height: 100%; min-width: 1280px; background: #fff; box-sizing: border-box; padding: 50px 0 0 200px; position: relative; }
.header { width: 100%; height: 50px; background: #fff; box-shadow: 0px 2px 2px 0px rgba(4, 4, 4, 0.09); position: absolute; left: 0; top: 0; z-index: 101; box-sizing: border-box; padding-left: 230px; }
.header .sys-logo { display: block; width: 422px; height: 48px; background: url(../images/sys-logo.png) no-repeat left 2px; }
.header .customer-info { height: 42px; line-height: 42px; font-size: 24px; margin-top: 2px; padding-right: 30px; }
